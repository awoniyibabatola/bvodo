{
  "name": "corporate-travel-backend",
  "version": "1.0.0",
  "description": "Backend API for Corporate Travel Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dev:prod-env": "NODE_ENV=production nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:migrate:production": "dotenv -e .env.production -- npx prisma migrate deploy",
    "db:push:dev": "dotenv -e .env.development -- npx prisma db push",
    "db:push:prod": "dotenv -e .env.production -- npx prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:dev": "dotenv -e .env.development -- ts-node prisma/seed.ts",
    "db:seed:prod": "dotenv -e .env.production -- ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "db:studio:prod": "dotenv -e .env.production -- npx prisma studio",
    "db:generate": "prisma generate",
    "env:use-dev": "copy .env.development .env",
    "env:use-prod": "copy .env.production .env"
  },
  "keywords": [
    "travel",
    "booking",
    "corporate",
    "api"
  ],
  "author": "Your Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@prisma/client": "^5.7.0",
    "@sendgrid/mail": "^8.1.6",
    "amadeus": "^11.0.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.14.0",
    "stripe": "^14.9.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.21",
    "@types/pdfkit": "^0.13.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
